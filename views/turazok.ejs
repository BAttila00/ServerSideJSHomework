<%- include('header') -%>
<%- include('indexMenu') -%>
<div class="container">
  <div class="row">
    <div class="col-8">
      <h2>Túrázók listája</h2>
      <table class="table table-striped table-bordered">
          <tr>
              <th>
                Név
              </th>
              <th>
                Megtett kilóméter
              </th>
            </tr>
        <% turazok.forEach(function(turazo){ %>
          <tr>
              <td>
                  <%= turazo.nev %>
              </td>
              <td>
                  <%= turazo.eletkor %>
              </td>
            </tr>
        <% }); %>
      </table>
    </div>
  </div>
</div>

<%- include('footer') -%>